#include <stdio.h>

int main() {
   int array[10];
   int head[3] = 9, 5, 8};
   int tail[3]  =  {24, 2, 97};
   int i;
   int k = 0;
 
   for(i = 0; i < 3; i++) {
      array[k] = head[i];
      k++;
   }
   for(i = 0; i < 3; i++) {
      array[k] = tail[i];
      k++;
   }
   printf("Tron 2 mang ta duoc mang nhu sau:\n");
   for(i = 0; i < 10; i++)
      printf(" %d ", array[i]);
   return 0;
}
